	background:$white;
}

* {
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
	-ms-box-sizing:border-box;
}

textarea, input, select { outline: none; }

.clear {
	clear:both;
}

a, a:hover, a:visited {
	color:inherit;
	text-decoration:none;
}


placeholder {
	color:$gray5;
} 
:-moz-placeholder, ::-moz-placeholder {
	color:$gray6;
}



