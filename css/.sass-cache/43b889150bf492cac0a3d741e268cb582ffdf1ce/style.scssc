	max-width:420px;
	width:100%;
	padding:24px 0 0 0;
}	

.homepage-top {
	height:96px;
	
}

.icon-search {
	position:absolute;
	top:0;
	left:4px;
	width:44px;
	height:44px;
	background:url(../img/icon-search.png) no-repeat;
	
	.top-search:hover & {
		background-position:0 -44px;
	}
}





.content-info {
	position:absolute;
	bottom:8px;
	left:0;
	color:$black;
	background:rgba($white, 1);
	padding:8px 12px;
	font-size:13px;
	margin: 0 8px;
	
	.double & {
		width:auto;
	}
	
	.main-content & {
		width:auto;
		font-size:24px;
		padding:16px 32px;
		
	}
}

.content-title {
	font-size:18px;
	line-height:1;
	.main-content & {
		font-size:44px;
	}
}

.content-cat {
	position:absolute;
	color:$white;
	text-shadow:1px 1px 4px rgba($black, .8);
	right:8px;
	top:8px;
	font-size:13px;
}

.icon-play {
	width:120px;
	height:120px;
	position:absolute;
	top:16px;
	left:16px;
	background:url(../img/icon-play.png) no-repeat;
}


@include keyframes(showHeaderLogo) {
	0% {
		@include translate3d(-105px, 0, 0);
	}
	100% {
		@include translate3d(0, 0, 0);
	}
}


.logo-sm {
	width:105px;
	height:16px;
	background:url(../img/logo-about-sm.png) no-repeat;
	float:left;
	margin:12px 32px 16px 12px;
	display:none;
	
	.dock-header  &	{
		display:block;
		@include css-animate(showHeaderLogo, .25s, 1, 0, ease-out);
	}
	
	
}


.dock-header {
	.home {
		display:none;
	}
	
	
	
}


@include keyframes(showHeaderSearch) {
	0% {
		@include translate3d(320px, 0, 0);
	}
	100% {
		@include translate3d(0, 0, 0);
	}
}
.header-search {
	display:none;
	width:320px;
	position:relative;
	float:right;
	margin:8px 16px 0 0;
	border-bottom:2px solid $white;
	
	
	
	@include shape;
	
	&:before {
		width:2px; 
		height:4px;
		background:$white;
		bottom:0;
		left:0;
	}
	
	&:after {
		width:2px; 
		height:4px;
		background:$white;
		bottom:0;
		right:0;
	}
	
	input {
		font-size:$font-med;
		height:24px;
		display:block;
		width:100%;
		border:none;
		background:none;
		padding-left:36px;
		color:$white;
	}
	
	.dock-header &	 {
		display:block;
		@include css-animate(showHeaderSearch, .25s, 1, 0, ease-out);
	}
	
	
}	

.icon-search-sm {
	position:absolute;
	top:4px;
	left:8px;
	width:16px;
	height:16px;
	background:url(../img/icon-search-sm.png) no-repeat;
	
}

.icon-menu {
	position:absolute;
	top:12px;
	left:14px;
	width:16px;
	height:16px;
	background:url(../img/icon-menu.png) no-repeat;
	
}

.header-btn {
	width:44px;
	border-left:1px solid $gray7;
	height:44px;
	float:right;
	position:relative;
	display:none;
	
	.icon-search-sm {
		top:12px;
		left:14px;
	}
	
	&:hover, &.active {
		background:$red1;
	}
}

.header-nav-inner {
	background:$black;
	position:relative;
	z-index:2;
	
	.header-nav:hover & {
		border-bottom:1px solid $gray8;
	}
}

.header-nav {
	
	color:$white;
	
	font-size:13px;
	position:fixed;
	height:32px;
	left:0;
	top:0;
	width:100%;
	z-index:1000;
	
	.header-nav-inner > span {
		display:block;
		height:32px;
		line-height:32px;
		float:left;
		padding:0 12px;
		cursor:pointer;
		color:$gray5;
		
		
		
		&.home {
			color:$red1;
		}

		&:hover, &.active {
			background:$red1;
			color:$white;
		}
	}
	
	@media only screen and (max-width : $breakp-mobile) {
	
		height:44px;
		.header-nav-inner > span {
			display:none;
		}
		
		.header-btn {
			display:block;
		}
		
		.logo-sm 	{
			display:block;
			@include css-animate(showHeaderLogo, .25s, 1, 0, ease-out);
		}
		
		
	}
	
	
}


@include keyframes(showSubNav) {
	0% {
		@include translate3d(0, -320px, 0);
	}
	100% {
		@include translate3d(0, 0, 0);
	}
}

.header-dropdown {
	height:320px;
	width:100%;
	
	bottom:320px;
	left:0;
	background:$black;
	
	box-shadow:0 2px 4px rgba($black, .8);
	display:none;
	
	&.show-subnav {
		@include css-animate(showSubNav, .25s, 1, 0, ease-out);
		display:block;
	}
	
	.subnav-big {
		display:none;
	}
	
	
	@media only screen and (min-width : $breakp-mobile) {
		
		.subnav-big {
			display:block;
		}
		
		.subnav-small {
			display:none;
		}
	}
}

.subnav-block {
	width:240px;
	float:left;
	margin:16px;
	
	.item {
		border:2px solid $gray8;
	}	
}


.sub-item {
	line-height:44px;
	margin:0 24px;
	border-bottom:1px solid $gray8;
	font-weight:bold;
	font-size:$font-big;
}

/*******FRAMEWORK********/
.header-dummy {
	height:44px;
}

.row {
	width:100%;
	margin: 0  auto;
	max-width:$breakp-wide;
	
	&:after {
		content:"";
		display:block;
		clear:both;
	}
}

.column {
	width:33.3%;
	float:left;
	word-break:keep-all ;
	
	&.double {
		width:66.6%;
	}
	
	@media only screen and (max-width : $breakp-reg) {
		width:50%;
		
		&:first-child {
			width:50%;
		}
		
		.left-rail &, .right-rail & {
			width:60%;
		}
		
		.left-rail &:last-child, .right-rail &:last-child {
			width:40%;
		}
		
		&.double {
			width:60%;
		}
	}
	
	@media only screen and (max-width : $breakp-mobile) {
		width:100%;
		
		&:first-child {
			width:100%;
		}
		
		.left-rail &:last-child, .right-rail &:last-child {
			width:100%;
		}
		
		&.double {
			width:100%;
		}
	}
}

img {
	width:100%;
}

.box {

	&.max-width-320 {
		max-width:320px;
		
		.left-rail & {
			float:right;
		}
		
		@media only screen and (max-width : $breakp-mobile) {
			max-width:none;
			.left-rail &  {
				float:none;
			}
		}
	}
	
	&.max-width-640 {
		max-width:800px;
	}
	
	
	
}

@include keyframes(itemFadeIn) {
	0% {
	
		opacity:0;
	
		@include translate3d(0, 88px, 0);

	}
	100% {
		opacity:1;
		@include translate3d(0, 0, 0);
	
	}
}

.item {
	position:relative;
	margin:8px;
	
	&.fadeIn-1 {
		@include css-animate(itemFadeIn, .5s, 1, 0, ease-out);
	}
	
	&.fadeIn-2 {
		@include css-animate(itemFadeIn, .35s, 1, 0, ease-out);
	}
	
	&.fadeIn-3 {
		@include css-animate(itemFadeIn, .75s, 1, 0, ease-out);
	}
}
	
.item-photo {
	&.fix-height {
		background-size:cover;
		background-position:center center;
		
		img {
			opacity:0;
		}
		
		@media only screen and (max-width : $breakp-mobile) {
			img {
				opacity:1;
			}	
			
			height:auto !important;
		}
	}
}

.item-title {
	background:$white;
	bottom:8px;
	left:8px;
	position:absolute;
	padding:8px 12px;
	font-size:20px;
	margin-right:8px;
	line-height:1;
	border-radius:1px;
}

.item-cat {
	position:absolute;
	top:8px;
	right:8px;
	font-weight:bold;
	color:$white;
	text-shadow:0 1px 4px rgba($black, .8);	
}

.title-big {
	font-size:54px;	
}

.left-rail {
	.double {
		float:right;
	}
}	

.half-column {
	width:50%;
	float:left;
	
	@media only screen and (max-width : $breakp-mobile) {
		width:100%;
		float:none;
	}
}

.list {
	padding:8px;
	
	&.fadeIn{
		@include css-animate(itemFadeIn, .35s, 1, 0, ease-out);
	}
}

.list-header {
	padding:8px 0 2px 0;
	font-size:20px;
	margin-top:0;
	border-bottom:1px solid $gray8;
}

.link-item {
	padding: 8px 0;
	/*border-bottom:1px solid $gray3;*/
	
	table {
		width:100%;
	}
	
	td {
		
		vertical-align:top;
		&.fix-width {
			width:1px;
		}
	}
}

.link-photo {
	img {
		width:120px;
	}
	
	
}

.link-title {
	padding:4px 4px 4px 12px;
	font-size:$font-big;
	
	a {
		color:$red1;
	}
}

.article-title {
	font-size:64px;
	padding:12px 8px 8px 32px;
	line-height:1.1;
	color:$black;
	
	@media only screen and (max-width : $breakp-mobile) {
		padding:16px 16px 8px 16px;
		font-size:36px;
	}
}

.article-body {
	font-size:$font-big;
	padding:16px 8px 16px 32px;
	line-height:1.4;
	
	p {
		margin-bottom:16px;
	}
	
	@media only screen and (max-width : $breakp-mobile) {
		padding:16px;
		font-size:$font-med;
	}
}
/*******FRAMEWORK END********/


