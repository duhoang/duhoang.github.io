	position:fixed;
	width:100%;
	height:52px;
	background:$black;
	bottom:0;
	left:0;
	z-index:10000;
	
	@media only screen and (min-width : $bp-narrow) {
		display:none;
	}
}

.footer-nav-item {
	height:52px;
	border-right:1px solid $gray8;
	width:25%;
	text-align:center;
	color:$gray5;
	font-size:$font-xsmall;
	float:left;
}

.footer-icon {
	height:32px;
	width:32px;
	border:2px solid $gray5;
	border-radius:100%;
	margin:4px auto;
}




.related-link-item {
	padding:8px 0;
}


.homepage-slideshow {
	@include shape;
	
/*	&:before {
		border:16px solid transparent;
		border-top-color:$blue-light;
		bottom:-30px;
		left:84px;
		z-index:8;
	}	*/
}

.hp-slide-scroll {
	width:2000px;
}

.hp-slide-inner {
	max-width:1518px;
	margin:16px auto 8px auto;	
	background:$gray1;
	padding:0 44px;
	border-top:1px dotted $gray3;
	border-bottom:1px dotted $gray3;
	white-space:nowrap;
	overflow:hidden;
	height:68px;
	

	
	&:after {
		content:"";
		display:block;
		clear:both;
	}
	
	@include shape;
	
	&:before {
		height:100%;
		width:44px;
		position:absolute;
		left:0;
		top:0;
		@include gradient-hori(rgba($white,1), 0%, rgba($white,0), 50%);
	}
	
	
	&:after {
		height:100%;
		width:44px;
		position:absolute;
		right:0;
		top:0;
		@include gradient-hori(rgba($white,0), 0%, rgba($white,1), 50%);
	}
	
	@media only screen and (max-width : $bp-narrow) {
		
		margin:0;
		
		
	}
	
}

.hp-heading {
	@include shape;
	position:absolute;
	bottom:0;
	left:0;
	width:100%;
	padding:2px 0;
	/*text-shadow:1px 1px 2px rgba($black, 1);*/
	text-align:center;
	line-height:1.2;
	color:$white;
	white-space:normal;
	
	
	.active & {
		
	}
	
	span {
		position:relative;
		z-index:5;
	}
	
	
	
	&:before {
		width:100%;
		height:100%;
		/*background:rgba($white, .5);*/
		
		top:0;
		left:0;
		z-index:1;
	}
	
	&:after {
		width:100%;
		height:100%;
		background:rgba($black, .5);
		top:0;
		left:0;
		z-index:2;
	}
	
	
}

.hp-slide-title {
	min-width:116px;
	height:68px;
	position:relative;
	float:left;
	display:block;
	font-size:12px;
	padding:8px 12px;
	margin-right:1px;
	font-weight:bold;
	line-height:0;
	color:$white;
	overflow:hidden;
	&.active, &:hover {
		background:$blue-light;
		color:$white;
		font-weight:bold;	
	}
}


.hp-slide-photo {
	width:88px;
	height:52px;
	
	position:relative;
	background-size:cover;
	background-position:center center;
	img {
		display:block;
	}	
}

.box {
	margin-bottom:1px;
	@media only screen and (min-width : $bp-regular) {
		width:50%;
		float:left;
		
		&.double {
			width:100%;
		}
		
		
		
	}
	
	@media only screen and (min-width : $bp-regular2) {
		width:33.3%;
		
		&.double {
			width:66.6%;
			
			
			&:nth-child(even) {
				float:right;
				margin-bottom:8px;	
			}
		}
	}	
}

.top-area-table {
	width:100%;
	
	.fix-width {
		width:1px;
	}
	
	td {
		vertical-align:top;
	}
}

.hp-left {
	width:32px;
	height:32px;
	background:url(../img/icons-sprite.png) -32px 0 no-repeat;
	position:absolute;
	top:50%;
	left:4px;
	margin-top:-16px;
	z-index:10;
}

.hp-right {
	width:32px;
	height:32px;
	background:url(../img/icons-sprite.png) -32px -32px no-repeat;
	position:absolute;
	top:50%;
	right:4px;
	z-index:4;
	margin-top:-16px;
	z-index:10;
}

.hp-editor-label {
	font-size:20px;
	white-space:normal;
	line-height:1.2;
	padding-top:4px;
}

.homepage-section {
	border-top:1px dotted $gray3;
	margin-top:16px;
	
}

.hp-section-heading {
	padding:16px 8px 8px 8px;
	font-size:24px;
	font-family: $robotoslab;
}

.share-block {
	font-size:14px;
	color:$blue-light;
	
	.tweet &, .blur & {
		position:absolute;
		bottom:16px;
		left:16px;
		
	}
	
	.tweet & {
		color:$white;
	}
}
.share-icon {
	width:24px;
	height:24px;
	background:url(../img/icons-sprite.png) -100px -4px no-repeat;
	display:inline-block;
	vertical-align:-6px;
	margin-right:4px;
	
}

.see-more {
	float:right;
	font-family:$helvetica;
	font-size:16px;
	color:$blue-light;
}
.see-more-icon {
	width:32px;
	height:32px;
	background:url(../img/icons-sprite.png) -64px -32px no-repeat;
	display:inline-block;
	vertical-align:-10px;
	margin-right:4px;
	
}

.content-title-bg {
	padding:16px;
	
	@media only screen and (min-width : $bp-regular) {
		padding-left:36px;
		
	}
}

.content-title {
	font-size:$font-xxbig;
	@media only screen and (min-width : $bp-narrow) {
		font-size:$font-xxxbig;
	}
	
	
	
}

.content-subtitle {
	font-size:$font-big;
	@media only screen and (min-width : $bp-narrow) {
		font-size:$font-xbig;
	}
}

.content-related-column {
	
	padding:16px;
	font-size:$font-med;
	
	@media only screen and (min-width : $bp-narrow) {
		
		.half-column {
			width:auto;
			float:none;
		}
	}
	
	@media only screen and (min-width :  $bp-narrow1) {
		
		.half-column {
			width:50%;
			float:left;
		}
	}
	
	@media only screen and (min-width : $bp-regular) {
		position:absolute; 
		right:8px;
		top:8px;
		width:240px;
		padding:0;
		
		.half-column {
			width:auto;
			float:none;
		}
	}
}	

.content-body {
	position:relative;
	
}

.content-body-inner {
	padding:16px;
	font-size:$font-med;
	
	@media only screen and (max-width : $bp-regular) {
		&[style] {
			min-height: inherit !important;
		}
		
	}
	
	
	
	@media only screen and (min-width : $bp-narrow) {
		font-size:$font-big;
		
		
	}
	
	@media only screen and (min-width : $bp-regular) {
		
		padding-right:260px;
	}
	
	
	@media only screen and (min-width : $bp-regular2) {
		padding-left:36px;
	}
}


.newsletter-sub-fields {

	input {
		display:block;
		width:100%;
		max-width:320px;
		float:left;
	}
}

.outline {
	border:1px dashed $gray4;
	padding:8px;
}

.related-box {
	li {
		list-style:disc outside;
		margin-left:16px;
	}
}

.middle-area {
	position:relative;
}

.bottom-area {
	position:relative;
	z-index:1;
	background:$gray1;
}

.share-module {
	background:rgba($white, .5);
	width:48px;
	position:fixed;
	right:0;
	z-index:10;
	
	
