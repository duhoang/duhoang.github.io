		color:$gray5;	
		font-size:14px;
	}
	
	a {
		display:block;
		
		&.t {
			color:$blue-light;
			font-size:14px;
			font-weight:bold;	
		}
		
		&.u {
			color:$gray5;
			text-decoration:underline;	
		}
	}
	
	@media only screen and (max-width : $bp-narrow) {
		
		float:none;
		border-top:1px solid $gray3;
