	color:$gray5;
	font-size:$font-xsmall;
	float:left;
}

.footer-icon {
	height:32px;
	width:32px;
	border:2px solid $gray5;
	border-radius:100%;
	margin:4px auto;
}


.share {
	position:absolute;
	top:8px;
	left:12px;
	margin-bottom:12px;
	img {
		width:auto;	
		margin:0 0 0 14px;
