.folded-corner {
	@include shape;
	position:absolute;
	right:0;
	top:0;
	border:11px solid $white;
		border-top-color:transparent;
		border-right-color:transparent;
		top:0;
		right:0;
	
	&:before {
		width:22px;
		height:22px;
		background:#ffeeb9;	
		top:-11px;
