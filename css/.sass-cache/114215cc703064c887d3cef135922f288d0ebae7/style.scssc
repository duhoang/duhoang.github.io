	font-size:16px;
	color:$blue-light;	
	line-height:44px;
	padding:0 16px 0 24px;
	@include shape;
	
	&:before {
		border:4px solid transparent;
		border-top:8px solid $blue-light;	
		top:50%;
		margin-top:-2px;
		left:8px;
	}
	
