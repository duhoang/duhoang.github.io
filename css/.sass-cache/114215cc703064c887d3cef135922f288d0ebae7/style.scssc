	}
	
	&:after {
		height:1px;
		width:100%;
		bottom:-1px;
		left:0;
		@include gradient-hori($blue-light, 0%, $purple, 100%);
	}
	
	table {
		width:100%;
	}
	
	td {
		
		vertical-align:top;
		
		&.fix-width {
			white-space:nowrap;
		}
	}
	
	
