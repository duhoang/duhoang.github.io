		margin:0 16px;
		
		
	}
	
	@media only screen and (min-width : $bp-regular) {
		max-width:640px;
		margin-left:100px;
		
		
		
	}
	
	@media only screen and (min-width : 1344px) {
		position:absolute;
		width:240px;
		right:0;
		top:16px;
			
		.half-column {
			width:auto;	
		}
		
		.item {
			margin:0 0 8px 0;	
		}
		
	}
	
}

