	z-index:10000;
	
	@media only screen and (min-width : $breakp-mobile) {
		display:none;
	}
}

.footer-nav-item {
	height:52px;
	border-right:1px solid $gray8;
	width:25%;
	text-align:center;
	color:$gray5;
	font-size:$font-xsmall;
	float:left;
}

.footer-icon {
	height:32px;
	width:32px;
	border:2px solid $gray5;
	border-radius:100%;
	margin:4px auto;
}

.date-box {
	height:60px;
	width:60px;
	border-radius:100%;
	text-align:center;
	float:left;
	margin:16px;	
	color:$blue-light;
	font-size:16px;
	padding:12px 4px;
	position:absolute;
	top:8px;
	left:12px;
	
	@include shape;
	
	&:before {
		width:100%;
		height:100%;
		position:absolute;
		border-radius:100%;
		@include gradient-hori($blue-light, 0%, $purple, 100%);
		top:0;
		left:0;
	}
	
	&:after {
		width:58px;
		height:58px;
		position:absolute;
		border-radius:100%;
		background:$white;
		top:1px;
		left:1px;
	}
	
	span {
		position:relative;
		z-index:2;	
	}
	
	
	
	@media only screen and (max-width : $breakp-reg) {
		position:relative;
		left:0;
	}
}

.lr-fix-width {
	position:relative;
	.double {
		float:none;
		width:100% !important;	
		padding-left:320px;
		
		@media only screen and (max-width : $breakp-mobile) {
			padding-left:0;
		}
		
		
	}
	
	.lf-wrap {
		position:absolute;
		top:0;
		left:0;	
		
		@media only screen and (max-width : $breakp-mobile) {
			position:relative;
		}
	}
	
	
}

.content-body {
	@include shape;
		
		&:before {
			width:1px;
			height:100%;
			left:328px;
			top:28px;
			background:$gray4;
			
			@media only screen and (max-width : $breakp-mobile) {
				display:none;
			}
		}	
		
		&:after {
			width:1px;
			height:100%;
			right:0;
			top:28px;
			background:$gray4;
			
			@media only screen and (max-width : $breakp-mobile) {
				display:none;
			}
		}	
}

.related-box {
	
	width:240px;
	right:16px;
	top:16px;
	padding-top:12px;
	
	@include shape;
		
	&:before {
		height:1px;
		width:100%;
		top:0;
		left:0;
		@include gradient-hori($blue-light, 0%, $purple, 100%);
	}
	
	position:absolute;
	.list-header {
		padding:0 8px 8px 0;	
	}
	
	.item {
		margin:0 0 8px 0;	
	}
	
	@media only screen and (max-width : $breakp-wide) {
		position:relative;
		width:auto;
		margin-left:100px;
		.item {
			width:240px;
			float:left;
			margin:8px;	
		}
	}
	
	@media only screen and (max-width : $breakp-reg) {
		margin-left:40px;
	}
